#!/usr/bin/env -S cargo run -- interpret
.registers {
    B, A ‚Üê 3x3 builtin (24, 210)
}

.define a-sub-1 {
    add A 209
}

    input "Number to modulus:" A
loop:
--    print "A is now" A
    add B 13
decrement:
    solved-goto B loop
