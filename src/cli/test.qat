#!/usr/bin/env -S cargo run -- interpret
.registers {
    B, A ‚Üê 3x3 builtin (24, 210)
}

.define MOD 13

input "Number to modulus:" A

loop:

print "A is now" A
add B $MOD

while not-solved A {
    solved-goto B loop
    add A 209
    add B 23
}

while not-solved B {
    add A 209
    add B 23
}

add A $MOD

halt "The modulus is" A
