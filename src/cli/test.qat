#!/usr/bin/env -S cargo run -- interpret
.registers {
    B, A ‚Üê 3x3 builtin (24, 210)
}

.define a-sub-1 {
    add A 209
}

input "Number to modulus:" A

loop:

print "A is now" A
add B 13

while not-solved A {
    solved-goto B loop
    $a-sub-1
    add B 23
}

while not-solved B {
    $a-sub-1
    add B 23
}

add A 13

halt "The modulus is" A
